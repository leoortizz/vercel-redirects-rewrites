{
  "redirects": [
    {
      "source": "/",
      "destination": "/dashboard",
      "permanent": false,
      "has": [
        {
          "type": "cookie",
          "key": "router-setting",
          "value": "redirect"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/home",
      "destination": "/"
    },
    {
      "source": "/db",
      "destination": "/dashboard",
      "has": [
        {
          "type": "cookie",
          "key": "router-setting",
          "value": "rewrite"
        }
      ]
    }
  ]
}
