{
  "redirects": [
    {
      "source": "/",
      "destination": "/dashboard",
      "permanent": false,
      "has": [
        {
          "type": "cookie",
          "key": "router-setting",
          "value": "redirect"
        },
        {
          "type": "query",
          "key": "noredirect",
          "value": "[^1]"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/home",
      "destination": "https://vercel-redirects-rewrites.vercel.app?noredirect=1"
    },
    {
      "source": "/raster",
      "destination": "https://raster.app"
    },
    {
      "source": "/",
      "destination": "https://vercel-redirects-rewrites.vercel.app/dashboard",
      "has": [
        {
          "type": "cookie",
          "key": "router-setting",
          "value": "rewrite"
        }
      ]
    }
  ]
}
